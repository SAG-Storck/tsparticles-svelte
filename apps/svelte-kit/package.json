    "@sveltejs/kit": "^1.5.0",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.26.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-particles": "^2.10.1",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.4.1",
    "tsparticles": "^2.10.1",
